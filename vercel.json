{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/auth.py",
            "use": "@vercel/python"
        },
        {
            "src": "api/ping.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api",
            "dest": "api/index.py"
        },
        {
            "src": "/api/signup",
            "dest": "api/auth.py"
        },
        {
            "src": "/api/login",
            "dest": "api/auth.py"
        },
        {
            "src": "/api/ping",
            "dest": "api/ping.py"
        }
    ]
}